}
.faq-box .panel-title > a:before {
    float: right !important;
    font-family: FontAwesome;
    content:"\f068";
    border: 1px solid;
    border-radius: 50%;
    /* font-size: 13px;
    padding: 8px 10px; */
    font-size: 10px;
    padding: 1px 3px;
    color: #000;
}
.faq-box .panel-title > a.collapsed:before {
    float: right !important;
    content:"\f067";
}
.faq-box .panel-title > a:hover, 
.panel-title > a:active, 
.panel-title > a:focus  {
    text-decoration:none;
}
